{
    "name": "danack/json-safe",
    "description": "safe json functions",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Danack",
            "email": "Danack@basereality.com"
        }
    ],
    "require": {
        "seld/jsonlint": "^1.7"
    },
    "require-dev": {
        "danack/coding-standard": "0.2.0",
        "phpunit/phpunit": "9.0.1",
        "phpstan/phpstan-strict-rules": "^0.11",
        "slevomat/coding-standard": "^4.8.4",
        "squizlabs/php_codesniffer": "^3.3.1",
        "yoast/yoastcs": "1.0"
    },
    "autoload": {
        "files": [
            "src/JsonSafe/functions.php"
        ],
        "psr-0": {
            "Params\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "JsonSafeTest\\": "test/"
        }
    }
}
